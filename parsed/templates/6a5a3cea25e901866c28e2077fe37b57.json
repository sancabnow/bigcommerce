{"components/common/forms/text":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\n    <label class=\"form-label\" for=\"{{id}}_input\">{{#if required}}<small class=\"required\">*</small>{{/if}}{{label}}\n    </label>\n    <input type=\"text\" id=\"{{id}}_input\" data-label=\"{{label}}\" name=\"{{name}}\" {{#if value}} value=\"{{value}}\"{{/if}} class=\"form-input\" aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\n</div>\n"}
